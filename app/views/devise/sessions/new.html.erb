<%= link_to get_started_path, class: "btn btn-primary" do %> Previous Page <% end %>


<div class="main-body">
  <div>
    <h2 class="main-h2">Log In</h2>
  </div>


  <div class="form-body">

    <%= form_for(resource, as: resource_name, url: session_path(resource_name), data: { turbo: false }) do |f| %>

      
      <div class= "form">
        <%= f.email_field :email, placeholder: :email, autofocus: true, autocomplete: "email" %>
      </div>
      
      
      <div class= "form">    
        <%= f.password_field :password, placeholder: :password, autocomplete: "current-password" %>
      </div>


      <div class="checks">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      </div>
    
    
      <div>
        <%= f.submit "Log in", class: "btn btn-primary" %>
      </div>
    
    
      <div class="other-actions">
        <%= render "devise/shared/links" %>
      </div>
    
    
    <% end %>
  </div>


</div>